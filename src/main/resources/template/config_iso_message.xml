<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE j8583-config PUBLIC "-//J8583//DTD CONFIG 1.0//EN" "http://j8583.sourceforge.net/j8583.dtd">
<j8583-config>

    <!-- Echo -->
    <parse type="0800">
        <field num="7" type="DATE10" length="10"/> <!-- Transmission Date and Time -->
        <field num="11" type="NUMERIC" length="6"/> <!-- Systems Trace Audit Number -->
        <field num="70" type="NUMERIC" length="3"/> <!-- Network Management Information Code -->
    </parse>

    <!-- Echo Response -->
    <parse type="0810">
        <field num="7" type="DATE10" length="10"/> <!-- Transmission Date and Time -->
        <field num="11" type="NUMERIC" length="6"/> <!-- Systems Trace Audit Number -->
        <field num="39" type="ALPHA" length="2"/> <!-- Response Code -->
        <field num="48" type="ALPHA" length="19"/> <!-- Additional Data â€“ Network Management -->
        <field num="70" type="NUMERIC" length="3"/> <!-- Network Management Information Code -->
    </parse>

    <parse type="0200">
        <field num="2" type="LLVAR"/>
        <field num="3" type="NUMERIC" length="6"/>
        <field num="4" type="AMOUNT" length="12"/>
        <field num="7" type="DATE10" length="10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="14" type="DATE4" length="4"/>
        <field num="18" type="NUMERIC" length="4"/>
        <field num="22" type="NUMERIC" length="3"/>
        <field num="25" type="NUMERIC" length="2"/>
        <field num="35" type="LLVAR" />
        <field num="37" type="ALPHA" length="12"/>
        <field num="41" type="ALPHA" length="8"/>
        <field num="49" type="NUMERIC" length="3"/>
    </parse>

    <parse type="0210">
        <field num="2" type="LLVAR"/>
        <field num="3" type="NUMERIC" length="6"/>
        <field num="4" type="AMOUNT" length="12"/>
        <field num="7" type="DATE10" length="10"/>
        <field num="11" type="NUMERIC" length="6"/>
        <field num="14" type="DATE4" length="4"/>
        <field num="18" type="NUMERIC" length="4"/>
        <field num="22" type="NUMERIC" length="3"/>
        <field num="25" type="NUMERIC" length="2"/>
        <field num="35" type="LLVAR" />
        <field num="37" type="ALPHA" length="12"/>
        <field num="39" type="NUMERIC" length="2"/>
        <field num="41" type="ALPHA" length="8"/>
        <field num="49" type="NUMERIC" length="3"/>
    </parse>

</j8583-config>
